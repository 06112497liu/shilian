<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbd.dao.KettleExecuteConfDao">
  <resultMap id="BaseResultMap" type="com.bbd.domain.KettleExecuteConfInfo">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="execute_seq" jdbcType="INTEGER" property="executeSeq" />
    <result column="execute_param" jdbcType="BLOB" property="executeParam" />
    <result column="data_source" jdbcType="VARCHAR" property="dataSource" />
    <result column="execute_type" jdbcType="VARCHAR" property="executeType" />
    <result column="data_flag" jdbcType="INTEGER" property="dataFlag" />
    <result column="execute_file" jdbcType="VARCHAR" property="executeFile" />
    
  </resultMap>
  <sql id="Base_Column_List">
    id,  execute_param, data_source, execute_type, data_flag, execute_seq
  </sql>
  <select id="selectConfByCondition" parameterType="java.util.Date" resultMap="BaseResultMap">
    SELECT
	      *
    FROM
	(
		SELECT DISTINCT 
      a.id,
      a.execute_param,
      a.data_source,
      a.execute_type,
      a.data_flag,
      a.execute_seq 
    FROM
      bbd_etl_kettle_conf a 
    WHERE a.data_flag = 1 
      AND a.id NOT IN 
      (SELECT DISTINCT 
        object_id 
      FROM
        bbd_etl_kettle_log 
      WHERE execute_end_time &gt; #{today,jdbcType=DATE} 
        AND execute_status = 'success')
	) k
    ORDER BY
	k.execute_seq ASC
  </select>
  
  
</mapper>