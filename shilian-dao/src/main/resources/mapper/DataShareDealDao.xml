<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbd.dao.DataShareDealDao">

  <select id="selectCompanyId" resultType="String">
    select nbxh as companyId from bbd_company_info where company_name = #{companyName}
  </select>
  
  <insert id="insertRelationData">
    INSERT INTO bbd_relation
    (company_id,has_central_enterprise,has_listed_company,update_time,gmt_create,company_name)
    VALUES
    (#{companyId},#{hasCentralEnterprise},#{hasListedCompany},now(),#{gmtCreate},#{companyName})
  </insert>


  
</mapper>